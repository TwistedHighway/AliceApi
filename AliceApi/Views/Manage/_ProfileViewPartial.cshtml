@model AliceApi.ViewModels.Manage.IndexViewModel


<div class="">
    <label>User Name:</label>
    @Html.Raw(Model.UserName)
</div>
<div class="">
    <label>Local User Name:</label>
    @Html.Raw(Model.MemberProfile.LocalUserName)
</div>
<div class="">
    <label>Public Handle:</label>
    @Html.Raw(Model.MemberProfile.PublicUserName)
</div>
<div class="">
    <label>Primary Email:</label>
    @Html.Raw(Model.MemberProfile.PrimaryEmail)
</div>
<div class="">
    <label>Last Updated:</label>
    @Html.Raw(Model.MemberProfile.DateUpdated)
</div>
<div class="form-actions">
    @Html.ActionLink("Edit", "EditProfile", "Manage",  null, new {@class="btn btn-default"})
</div>
